{
  "name": "gitget",
  "version": "0.0.14",
  "description": "ðŸ“¦ Clone GitHub repos fast and easy.",
  "main": "lib/api.js",
  "types": "lib/api.d.ts",
  "bin": {
    "gitget": "./lib/bin.js"
  },
  "scripts": {
    "start": "npm run dev",
    "dev": "rimraf lib && tsc --watch",
    "build": "tsc --project tsconfig.json",
    "test": "jest --coverage",
    "clean": "rimraf lib",
    "format": "prettier --check src/**",
    "format:write": "prettier --write src/**",
    "lint": "eslint src/**/*",
    "lint:fix": "eslint --fix src/**/*",
    "release": "npx publish-cli",
    "preReleaseHook": "clean format lint build test",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yandeu/gitget.git"
  },
  "keywords": [
    "git",
    "github",
    "clone",
    "repo",
    "repository"
  ],
  "author": "Yannick Deubel (https://github.com/yandeu)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yandeu/gitget/issues"
  },
  "homepage": "https://github.com/yandeu/gitget#readme",
  "devDependencies": {
    "@types/node": "^16.11.11",
    "@types/rimraf": "^3.0.0",
    "@types/tar": "^6.1.1",
    "@yandeu/eslint-config": "^0.0.3",
    "@yandeu/prettier-config": "^0.0.2",
    "jest": "^27.4.3",
    "prettier": "^2.2.1",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "rimraf": "^3.0.2",
    "tar": "^6.1.0"
